local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Viride Hub | Break In 2",
   LoadingTitle = "Viride Hub | Break In 2",
   LoadingSubtitle = "By MishaRuz13",
   Theme = "DarkBlue", -- Check https://docs.sirius.menu/rayfield/configuration/themes

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Big Hub"
   },
})

local MainTab = Window:CreateTab("Main | In Lobby", nil)
local Section = MainTab:CreateSection("Free Gamepass")

local Paragraph = MainTab:CreateParagraph({Title = "Tip", Content = "The Free Hacker's Phone Doesn't Work"})
local Button = MainTab:CreateButton({
   Name = "Free Hacker",
   Callback = function()
        local args = {
        [1] = "Phone",
        [2] = false,
        [3] = false
    }
    game:GetService("ReplicatedStorage").RemoteEvents.OutsideRole:FireServer(unpack(args))
   end,
})
local Button = MainTab:CreateButton({
   Name = "Free Nerd",
   Callback = function()
    local args = {
        [1] = "Book",
        [2] = true,
        [3] = false
}
    game:GetService("ReplicatedStorage").RemoteEvents.OutsideRole:FireServer(unpack(args))
   end,
})

local InGameTab = Window:CreateTab("In Game", nil)
local Section = InGameTab:CreateSection("Give Item")

local Button = InGameTab:CreateButton({
   Name = "Apple",
   Callback = function()
    local args = {
    [1] = "Apple"
}
game:GetService("ReplicatedStorage").Events.GiveTool:FireServer(unpack(args))
end,
})
